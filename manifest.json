{
  "manifest_version": 3,
  "name": "extension",
  "version": "1.0",
  "description": "hello, world",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.ico"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
